<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Title>KoiVM Processor</Title>
    <Description>KoiVM distrbution processor</Description>
    <UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="Confuser.Core">
      <HintPath>$(ConfuserInstallationDir)\Confuser.Core.dll</HintPath>
    </Reference>
    <Reference Include="Confuser.DynCipher">
      <HintPath>$(ConfuserInstallationDir)\Confuser.DynCipher.dll</HintPath>
    </Reference>
